<sDrawer>:
<ContentNavigationDrawer>:

Screen:

    MDNavigationLayout:

        ScreenManager:
            id: screen_manager
            Screen:
                name: "enter"
                FitImage:
                    size_hint: 1, 1
                    source: "G:/TravelGo/Photo/signin.png"
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDTextField:
                    id: login
                    hint_text_color_normal: "Black"
                    size_hint: .73, .09
                    width: "400dp"
                    hint_text: "Login"
                    pos_hint: {"center_x": .5, "center_y": .55}
                MDTextField:
                    id: password
                    hint_text_color_normal: "black"
                    size_hint: .73, .09
                    width: "400dp"
                    hint_text: "Password"
                    pos_hint: {"center_x": .5, "center_y": .45}
                MDFlatButton:
                    text: ""
                    font_size: "5sp"
                    pos_hint: {"center_x": .58, "center_y": .135}
                    on_press:
                        app.screen("creatbs")
            Screen:
                name: "home"
                MDBottomNavigation:
                    panel_color: .2, .2, .2, 1
                    MDBottomNavigationItem:
                        name: 'screen 1'
                        text: 'Туры'
                        icon: 'wallet-travel'

                    MDBottomNavigationItem:
                        name: 'screen 2'
                        text: 'Отели'
                        icon: 'home-search-outline'
                        MDIconButton:
                            icon: "arrow-left"
                            pos_hint: {"center_x": .1, "center_y": .9}
                            on_release:
                                app.screen("name")
                        MDTextField:
                            id: poisks
                            hint_text: "Поиск"
                            size_hint: .6, None
                            pos_hint: {"center_x": .51, "center_y": .9}
                        MDIconButton:
                            icon: "search-web"
                            pos_hint: {"center_x": .8, "center_y": .9}
                            on_release:
                                app.show_example_grid_bottom_sheet()
                        MDIconButton:
                            id: acc
                            icon: "account-circle-outline"
                            font_size: "60sp"
                            pos_hint: {"center_x": .9, "center_y": .909}
                            on_release:
                                app.account()


                    MDBottomNavigationItem:
                        name: 'screen 3'
                        text: 'Профиль'
                        icon: 'account-settings-outline'

            Screen:
                name: "creatbs"
                FitImage:
                    size_hint: 1, 1
                    source: "G:/TravelGo/Photo/signup.png"
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDTextField:
                    id: log
                    hint_text_color_normal: "Black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Login"
                    pos_hint: {"center_x": .49, "center_y": .635}
                MDTextField:
                    id: pase
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Password"
                    pos_hint: {"center_x": .49, "center_y": .535}
                MDTextField:
                    id: email
                    hint_text_color_normal: "Black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "E-mail"
                    pos_hint: {"center_x": .49, "center_y": .435}
                MDTextField:
                    id: name
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Name"
                    pos_hint: {"center_x": .49, "center_y": .335}
                MDFlatButton:
                    text: "You want to be our partner?"
                    theme_text_color: "Custom"
                    text_color: 0.65, 0.65, 0.65, 1
                    pos_hint: {"center_x": .5, "center_y": .12}
                    on_release:
                        app.screen("")
                MDFlatButton:
                    text: ""
                    pos_hint: {"center_x": .5, "center_y": .2}
                    on_release:
                        app.registration()
            Screen:
                name: "admin"

        MDNavigationDrawer:
            id: nav_drawer

            ContentNavigationDrawer:
                screen_manager: screen_manager
                nav_drawer: nav_drawer