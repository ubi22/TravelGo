<sDrawer>:
<ContentNavigationDrawer>:
Screen:

    MDNavigationLayout:

        ScreenManager:
            id: screen_manager
            Screen:
                name: "enter"
                FitImage:
                    size_hint: 1, 1
                    source: "G:/TravelGo/Photo/In.png"
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDTextField:
                    id: login
                    hint_text_color_normal: "Black"
                    size_hint: .73, .09
                    width: "400dp"
                    hint_text: "Login"
                    pos_hint: {"center_x": .5, "center_y": .55}
                MDTextField:
                    id: password
                    hint_text_color_normal: "black"
                    size_hint: .73, .09
                    width: "400dp"
                    hint_text: "Password"
                    pos_hint: {"center_x": .5, "center_y": .45}
                MDFlatButton:
                    text: ""
                    font_size: "5sp"
                    pos_hint: {"center_x": .5, "center_y": .275}
                    on_press:
                        app.log_in()
                MDFlatButton:
                    text: ""
                    font_size: "5sp"
                    pos_hint: {"center_x": .58, "center_y": .135}
                    on_press:
                        app.screen("creatbs")
            Screen:
                name: "home"
                MDBottomNavigation:
                    panel_color: .2, .2, .2, 1
                    MDBottomNavigationItem:
                        name: 'screen 1'
                        text: 'Туры'
                        icon: 'wallet-travel'
                        FitImage:
                            source: "G:/TravelGo/Photo/camera.png"
                    MDBottomNavigationItem:
                        name: 'screen 2'
                        text: 'Отели'
                        icon: 'home-search-outline'
                        FitImage:
                            source: "G:/TravelGo/Photo/camera.png"
                        MDIconButton:
                            icon: "arrow-left"
                            pos_hint: {"center_x": .1, "center_y": .9}
                            on_release:
                                app.screen("name")
                        MDTextField:
                            id: poisks
                            hint_text: "Поиск"
                            size_hint: .6, None
                            pos_hint: {"center_x": .51, "center_y": .9}
                        MDIconButton:
                            icon: "search-web"
                            pos_hint: {"center_x": .8, "center_y": .9}
                            on_release:
                                app.show_example_grid_bottom_sheet()
                        MDIconButton:
                            id: acc
                            icon: "account-circle-outline"
                            font_size: "60sp"
                            pos_hint: {"center_x": .9, "center_y": .909}
                            on_release:
                                app.account()


                    MDBottomNavigationItem:
                        name: 'screen 3'
                        text: 'Профиль'
                        icon: 'account-settings-outline'
                        FitImage:
                            source: "G:/TravelGo/Photo/camera.png"
                        BoxLayout:
                            orientation: "vertical"
                            padding: "6dp"
                            spacing: "6dp"
                            MDIconButton:
                                id: iconhome
                                pos_hint: {"center_x": .5, "center_y": .9}
                                user_font_size: "128sp"
                                icon: "account-circle-outline"
                                on_release: app.file_manager_open(0)

                            MDLabel:
                                text: "Васильев Андрей Витальевич"
                                font_style: "Button"
                                size_hint_y: None
                                halign: "center"
                                font_style: "H4"

                            ScrollView:

                                MDList:
                                    OneLineAvatarListItem:
                                        on_release: app.screen("settingadmin")
                                        text: "Настройки профиля"
                                        IconLeftWidget:
                                            icon: "cog"
                                    OneLineAvatarListItem:
                                        on_release: app.show_ate_picker()
                                        text: "Отзывы"
                                        IconLeftWidget:
                                            icon: "message-text-outline"

            Screen:
                name: "creatbs"
                FitImage:
                    size_hint: 1, 1
                    source: "G:/TravelGo/Photo/Up.png"
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDTextField:
                    id: log
                    hint_text_color_normal: "Black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Login"
                    pos_hint: {"center_x": .49, "center_y": .635}
                MDTextField:
                    id: pase
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Password"
                    pos_hint: {"center_x": .49, "center_y": .535}
                MDTextField:
                    id: email
                    hint_text_color_normal: "Black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "E-mail"
                    helper_text: "user@gmail.com"
                    helper_text_mode: "on_focus"
                    pos_hint: {"center_x": .49, "center_y": .435}
                MDTextField:
                    id: name
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Name"
                    helper_text: "Surname Name Patronymic"
                    helper_text_mode: "on_focus"
                    pos_hint: {"center_x": .49, "center_y": .335}
                MDFlatButton:
                    text: "You want to be our partner?"
                    theme_text_color: "Custom"
                    text_color: 0.65, 0.65, 0.65, 1
                    pos_hint: {"center_x": .5, "center_y": .12}
                    on_release:
                        app.screen("partner")
                MDFlatButton:
                    text: ""
                    pos_hint: {"center_x": .5, "center_y": .2}
                    on_release:
                        app.registrationuser()
            Screen:
                name: "partner"
                FitImage:
                    size_hint: 1, 1
                    source: "G:/TravelGo/Photo/partner.png"
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDTextField:
                    id: namecom
                    hint_text_color_normal: "Black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Name Company"
                    pos_hint: {"center_x": .49, "center_y": .59}
                MDTextField:
                    id: logincom
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Login"
                    pos_hint: {"center_x": .49, "center_y": .49}
                MDTextField:
                    id: pasecom
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Password"
                    pos_hint: {"center_x": .49, "center_y": .39}
                MDFlatButton:
                    text: ""
                    pos_hint: {"center_x": .5, "center_y": .26}
                    on_release:
                        app.registrationadmin()
            Screen:
                name: "admin"
                MDBottomNavigation:
                    panel_color: .2, .2, .2, 1
                    MDBottomNavigationItem:
                        name: 'screen 1'
                        text: 'Туры'
                        icon: 'briefcase-plus-outline'
                        FitImage:
                            source: "G:/TravelGo/Photo/camera.png"
                        ScrollView:
                            scroll_timeout : 150
                            MDList:
                                id: md_list
                                padding: 0
                                BoxLayout:
                                    orientation: "horizontal"
                                    FitImage:
                                        source: "G:/TravelGo/Photo/up.png"
                                        size_hint: 0.25, 0.85
                                    BoxLayout:
                                        orientation: "vertical"

                        FloatLayout:
                            MDIconButton:
                                icon: "plus-circle-outline"
                                pos_hint: {"center_x": .9, "center_y": .09}
                                user_font_size: "50sp"
                                on_release:
                                    app.screen("createtravel")

                    MDBottomNavigationItem:
                        name: 'screen 2'
                        text: 'Отели'
                        icon: 'domain-plus'
                        FitImage:
                            source: "G:/TravelGo/Photo/camera.png"

                    MDBottomNavigationItem:
                        name: 'screen 3'
                        text: 'Профиль'
                        icon: 'account-settings-outline'
                        FitImage:
                            source: "G:/TravelGo/Photo/camera.png"
                        BoxLayout:
                            orientation: "vertical"
                            padding: "6dp"
                            spacing: "6dp"
                            MDIconButton:
                                id: iconadmin
                                pos_hint: {"center_x": .5, "center_y": .9}
                                user_font_size: "128sp"
                                icon: "account-circle-outline"
                                on_release: app.file_manager_open(1)

                            MDLabel:
                                text: "Васильев Андрей Витальевич"
                                font_style: "Button"
                                size_hint_y: None
                                halign: "center"
                                font_style: "H4"

                            ScrollView:

                                MDList:
                                    OneLineAvatarListItem:
                                        on_release: app.screen("settingadmin")
                                        text: "Настройки профиля"
                                        IconLeftWidget:
                                            icon: "cog"
                                    OneLineAvatarListItem:
                                        on_release: app.show_ate_picker()
                                        text: "Отзывы"
                                        IconLeftWidget:
                                            icon: "message-text-outline"

            Screen:
                name: "settingadmin"

            MDScreen:
                name: "createtravel"
                MDTextField:
                    id: namet
                    hint_text: "Название тура"
                    fill_color: 0, 0, 0, .4
                    pos_hint: {"center_x": .5, "center_y": .75}
                    mode: "fill"
                MDTextField:
                    id: op
                    hint_text: "Описание"
                    max_height: "100dp"
                    mode: "fill"
                    pos_hint: {"center_x": .5, "center_y": .64}
                    fill_color: 0, 0, 0, .4
                    multiline: True
                FitImage:
                    id: h1
                    pos_hint: {"center_x": .1, "center_y": .5}
                    source: "G:/TravelGo/Photo/camera.png"
                    size_hint: .15, .1
                FitImage:
                    id: h2
                    pos_hint: {"center_x": .3, "center_y": .5}
                    source: "G:/TravelGo/Photo/camera.png"
                    size_hint: .15, .1
                FitImage:
                    id: h3
                    pos_hint: {"center_x": .5, "center_y": .5}
                    source: "G:/TravelGo/Photo/camera.png"
                    size_hint: .15, .1
                FitImage:
                    id: h4
                    pos_hint: {"center_x": .7, "center_y": .5}
                    source: "G:/TravelGo/Photo/camera.png"
                    size_hint: .15, .1
                FitImage:
                    id: h5
                    pos_hint: {"center_x": .9, "center_y": .5}
                    source: "G:/TravelGo/Photo/camera.png"
                    size_hint: .15, .1
                MDIconButton:
                    icon: "camera"
                    on_release: app.file_manager_open(2)
                    pos_hint: {"center_x": .1, "center_y": .5}
                    user_font_size: "50sp"
                MDIconButton:
                    icon: "camera"
                    on_release: app.file_manager_open(3)
                    pos_hint: {"center_x": .3, "center_y": .5}
                    user_font_size: "50sp"
                MDIconButton:
                    icon: "camera"
                    on_release: app.file_manager_open(4)
                    pos_hint: {"center_x": .5, "center_y": .5}
                    user_font_size: "50sp"
                MDIconButton:
                    icon: "camera"
                    on_release: app.file_manager_open(5)
                    pos_hint: {"center_x": .7, "center_y": .5}
                    user_font_size: "50sp"
                MDIconButton:
                    icon: "camera"
                    on_release: app.file_manager_open(6)
                    pos_hint: {"center_x": .9, "center_y": .5}
                    user_font_size: "50sp"
                MDTextField:
                    id: phonea
                    hint_text: "Номер для связи"
                    mode: "fill"
                    pos_hint: {"center_x": .5, "center_y": .35}
                    fill_color: 0, 0, 0, .4
                MDTextField:
                    id: emaila
                    hint_text: "E-mail для связи"
                    mode: "fill"
                    pos_hint: {"center_x": .5, "center_y": .25}
                    fill_color: 0, 0, 0, .4
                FitImage:
                    id: camera
                    pos_hint: {"center_x": .5, "center_y": .9}
                    source: "G:/TravelGo/Photo/camera.png"
                    size_hint: 1, .21
                MDIconButton:
                    icon: "camera"
                    on_release:
                        app.file_manager_open(7)
                    user_font_size: "64sp"
                    pos_hint: {"center_x": .5, "center_y": .9}
                MDTextField:
                    id: sele
                    mode: "fill"
                    pos_hint: {"center_x": .5, "center_y": .15}
                    fill_color: 0, 0, 0, .4
                    hint_text: "Ценна"

                MDFillRoundFlatIconButton:
                    icon: "content-save-all-outline"
                    md_bg_color: .3, .71, .18
                    text: "Сохранять"
                    pos_hint: {"center_x": .5, "center_y": .05}
                    on_release:
                        app.create()

        MDNavigationDrawer:
            id: nav_drawer

            ContentNavigationDrawer:
                screen_manager: screen_manager
                nav_drawer: nav_drawer