<sDrawer>:
<ContentNavigationDrawer>:

Screen:

    MDNavigationLayout:

        ScreenManager:
            id: screen_manager
            Screen:
                name: "enter"
                FitImage:
                    size_hint: 1, 1
                    source: "C:/Users/Admin/PycharmProjects/TravelGo/Photo/In.png"
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDTextField:
                    id: login
                    hint_text_color_normal: "Black"
                    size_hint: .73, .09
                    width: "400dp"
                    hint_text: "Login"
                    pos_hint: {"center_x": .5, "center_y": .55}
                MDTextField:
                    id: password
                    hint_text_color_normal: "black"
                    size_hint: .73, .09
                    width: "400dp"
                    hint_text: "Password"
                    pos_hint: {"center_x": .5, "center_y": .45}
                MDFlatButton:
                    text: ""
                    font_size: "5sp"
                    pos_hint: {"center_x": .5, "center_y": .275}
                    on_press:
                        app.log_in()
                MDFlatButton:
                    text: ""
                    font_size: "5sp"
                    pos_hint: {"center_x": .58, "center_y": .135}
                    on_press:
                        app.screen("creatbs")
            Screen:
                name: "home"
                MDBottomNavigation:
                    panel_color: .2, .2, .2, 1
                    MDBottomNavigationItem:
                        name: 'screen 1'
                        text: 'Туры'
                        icon: 'wallet-travel'

                    MDBottomNavigationItem:
                        name: 'screen 2'
                        text: 'Отели'
                        icon: 'home-search-outline'
                        MDIconButton:
                            icon: "arrow-left"
                            pos_hint: {"center_x": .1, "center_y": .9}
                            on_release:
                                app.screen("name")
                        MDTextField:
                            id: poisks
                            hint_text: "Поиск"
                            size_hint: .6, None
                            pos_hint: {"center_x": .51, "center_y": .9}
                        MDIconButton:
                            icon: "search-web"
                            pos_hint: {"center_x": .8, "center_y": .9}
                            on_release:
                                app.show_example_grid_bottom_sheet()
                        MDIconButton:
                            id: acc
                            icon: "account-circle-outline"
                            font_size: "60sp"
                            pos_hint: {"center_x": .9, "center_y": .909}
                            on_release:
                                app.account()


                    MDBottomNavigationItem:
                        name: 'screen 3'
                        text: 'Профиль'
                        icon: 'account-settings-outline'

            Screen:
                name: "creatbs"
                FitImage:
                    size_hint: 1, 1
                    source: "C:/Users/Admin/PycharmProjects/TravelGo/Photo/Up.png"
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDTextField:
                    id: log
                    hint_text_color_normal: "Black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Login"
                    pos_hint: {"center_x": .49, "center_y": .635}
                MDTextField:
                    id: pase
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Password"
                    pos_hint: {"center_x": .49, "center_y": .535}
                MDTextField:
                    id: email
                    hint_text_color_normal: "Black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "E-mail"
                    helper_text: "user@gmail.com"
                    helper_text_mode: "on_focus"
                    pos_hint: {"center_x": .49, "center_y": .435}
                MDTextField:
                    id: name
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Name"
                    helper_text: "Surname Name Patronymic"
                    helper_text_mode: "on_focus"
                    pos_hint: {"center_x": .49, "center_y": .335}
                MDFlatButton:
                    text: "You want to be our partner?"
                    theme_text_color: "Custom"
                    text_color: 0.65, 0.65, 0.65, 1
                    pos_hint: {"center_x": .5, "center_y": .12}
                    on_release:
                        app.screen("partner")
                MDFlatButton:
                    text: ""
                    pos_hint: {"center_x": .5, "center_y": .2}
                    on_release:
                        app.registrationuser()
            Screen:
                name: "partner"
                FitImage:
                    size_hint: 1, 1
                    source: "C:/Users/Admin/PycharmProjects/TravelGo/Photo/partner.png"
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDTextField:
                    id: namecom
                    hint_text_color_normal: "Black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Name Company"
                    pos_hint: {"center_x": .49, "center_y": .59}
                MDTextField:
                    id: logincom
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Login"
                    pos_hint: {"center_x": .49, "center_y": .49}
                MDTextField:
                    id: pasecom
                    hint_text_color_normal: "black"
                    size_hint: .68, .07
                    width: "400dp"
                    hint_text: "Password"
                    pos_hint: {"center_x": .49, "center_y": .39}
                MDFlatButton:
                    text: ""
                    pos_hint: {"center_x": .5, "center_y": .26}
                    on_release:
                        app.registrationadmin()
            Screen:
                name: "admin"
                MDBottomNavigation:
                    panel_color: .2, .2, .2, 1
                    MDBottomNavigationItem:
                        name: 'screen 1'
                        text: 'Туры'
                        icon: 'briefcase-plus-outline'
                        ScrollView:
                            scroll_timeout : 150
                            MDList:
                                id: md_list
                                padding: 0
                                MDFloatLayout:
                                    FitImage:
                                        source: "C:/Users/Admin/PycharmProjects/TravelGo/Photo/camera.png"
                                        size_hint:  1, .2
                                        pos_hint: {"center_x": .5, "center_y": .9}
                                    MDIconButton:
                                        icon: "camera-outline"
                                        pos_hint: {"center_x": .5, "center_y": .895}
                                        user_font_size: "75sp"
                                        on_release:
                                            app.screen("enter")
                                BoxLayout:
                                    orientation: "vertical"
                                    BoxLayout:
                                        orientation: "horizontal"
                                        MDTextField:
                                            hint_text: "Название Тура"
                                            mode: "fill"
                                            fill_color: 0, 0, 0, .4
                                    BoxLayout:
                                        orientation: "horizontal"
                                        MDTextField:
                                            hint_text: "Описание"
                                            max_height: "200dp"
                                            mode: "fill"
                                            fill_color: 0, 0, 0, .4
                                            multiline: True
                                    BoxLayout:
                                        orientation: "horizontal"
                                        MDTextField:
                                            hint_text: "Номер для связи"
                                            mode: "fill"
                                            fill_color: 0, 0, 0, .4
                                    BoxLayout:
                                        orientation: "horizontal"
                                        MDTextField:
                                            hint_text: "Почта для связи"
                                            mode: "fill"
                                            fill_color: 0, 0, 0, .4
                                    BoxLayout:
                                        orientation: "horizontal"
                                        MDTextField:
                                            hint_text: "Почта для связи"
                                            mode: "fill"
                                            fill_color: 0, 0, 0, .4
                                    BoxLayout:
                                        orientation: "horizontal"
                                        MDTextField:
                                            hint_text: "Почта для связи"
                                            mode: "fill"
                                            fill_color: 0, 0, 0, .4
                    MDBottomNavigationItem:
                        name: 'screen 2'
                        text: 'Отели'
                        icon: 'domain-plus'



                    MDBottomNavigationItem:
                        name: 'screen 3'
                        text: 'Профиль'
                        icon: 'account-settings-outline'

        MDNavigationDrawer:
            id: nav_drawer

            ContentNavigationDrawer:
                screen_manager: screen_manager
                nav_drawer: nav_drawer